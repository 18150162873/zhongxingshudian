CREATE TABLE API_RECEIPT
   (SERIALKEY NUMBER , 
  WHSEID NVARCHAR2(30) DEFAULT USER, 
  RECEIPTKEY NVARCHAR2(10) , 
  EXTERNRECEIPTKEY NVARCHAR2(50) DEFAULT ' ' , 
  RECEIPTGROUP NVARCHAR2(20) DEFAULT ' ' , 
  STORERKEY NVARCHAR2(15) DEFAULT ' ' , 
  RECEIPTDATE DATE DEFAULT SYS_EXTRACT_UTC(SYSTIMESTAMP), 
  POKEY NVARCHAR2(50) DEFAULT ' ', 
  CARRIERKEY NVARCHAR2(15), 
  CARRIERNAME NVARCHAR2(45), 
  CARRIERADDRESS1 NVARCHAR2(45), 
  CARRIERADDRESS2 NVARCHAR2(45), 
  CARRIERCITY NVARCHAR2(45), 
  CARRIERSTATE NVARCHAR2(25), 
  CARRIERZIP NVARCHAR2(18), 
  CARRIERREFERENCE NVARCHAR2(18), 
  WAREHOUSEREFERENCE NVARCHAR2(18) DEFAULT ' ', 
  ORIGINCOUNTRY NVARCHAR2(30), 
  DESTINATIONCOUNTRY NVARCHAR2(30), 
  VEHICLENUMBER NVARCHAR2(18), 
  VEHICLEDATE DATE, 
  PLACEOFLOADING NVARCHAR2(18), 
  PLACEOFDISCHARGE NVARCHAR2(30), 
  PLACEOFDELIVERY NVARCHAR2(30), 
  INCOTERMS NVARCHAR2(10), 
  TERMSNOTE NVARCHAR2(18), 
  CONTAINERKEY NVARCHAR2(18), 
  SIGNATORY NVARCHAR2(18), 
  PLACEOFISSUE NVARCHAR2(18), 
  OPENQTY NUMBER(22,5) DEFAULT 0, 
  FORTE_FLAG NVARCHAR2(6) DEFAULT 'I' , 
  STATUS NVARCHAR2(10) DEFAULT '0' , 
  EFFECTIVEDATE DATE, 
  CONTAINERTYPE NVARCHAR2(20), 
  CONTAINERQTY NUMBER(22,5), 
  BILLEDCONTAINERQTY NUMBER(22,5), 
  TRANSPORTATIONMODE NVARCHAR2(30), 
  EXTERNALRECEIPTKEY2 NVARCHAR2(20), 
  SUSR1 NVARCHAR2(30) DEFAULT '20', 
  SUSR2 NVARCHAR2(30), 
  SUSR3 NVARCHAR2(30), 
  SUSR4 NVARCHAR2(30), 
  SUSR5 NVARCHAR2(30), 
  TYPE NVARCHAR2(10) DEFAULT '1' , 
  RMA NVARCHAR2(30), 
  EXPECTEDRECEIPTDATE DATE, 
  ALLOWAUTORECEIPT NVARCHAR2(1), 
  CLOSEDDATE DATE, 
  TRACKINVENTORYBY NVARCHAR2(1) DEFAULT '0', 
  CARRIERCOUNTRY NVARCHAR2(30), 
  CARRIERPHONE NVARCHAR2(18), 
  DRIVERNAME NVARCHAR2(25), 
  TRAILERNUMBER NVARCHAR2(20), 
  TRAILEROWNER NVARCHAR2(25), 
  TRAILERTYPE NVARCHAR2(10), 
  ARRIVALDATETIME DATE, 
  LOTTABLEMATCHREQUIRED NVARCHAR2(1) DEFAULT '0' , 
  ADVICENUMBER NVARCHAR2(30), 
  ADVICEDATE DATE DEFAULT SYS_EXTRACT_UTC(SYSTIMESTAMP), 
  PACKINGSLIPNUMBER NVARCHAR2(50), 
  RECEIPTID NVARCHAR2(32), 
  SUPPLIERCODE NVARCHAR2(15), 
  SUPPLIERNAME NVARCHAR2(45), 
  SHIPFROMADDRESSLINE1 NVARCHAR2(45), 
  SHIPFROMADDRESSLINE2 NVARCHAR2(45), 
  SHIPFROMADDRESSLINE3 NVARCHAR2(45), 
  SHIPFROMADDRESSLINE4 NVARCHAR2(45), 
  SHIPFROMCITY NVARCHAR2(45), 
  SHIPFROMSTATE NVARCHAR2(25), 
  SHIPFROMZIP NVARCHAR2(18), 
  SHIPFROMISOCOUNTRY NVARCHAR2(3), 
  SHIPFROMCONTACT NVARCHAR2(30), 
  SHIPFROMPHONE NVARCHAR2(18), 
  SHIPFROMEMAIL NVARCHAR2(60), 
  SCHEDULEDSHIPDATE DATE, 
  ACTUALSHIPDATE DATE, 
  GROSSWEIGHT NUMBER(22,5), 
  TOTALVOLUME NUMBER(22,5), 
  SOURCELOCATION NVARCHAR2(64), 
  SOURCEVERSION NVARCHAR2(22), 
  REFERENCETYPE NVARCHAR2(64), 
  REFERENCEDOCUMENT NVARCHAR2(64), 
  REFERENCELOCATION NVARCHAR2(64), 
  REFERENCEVERSION NVARCHAR2(20), 
  DOOR NVARCHAR2(10), 
  APPOINTMENTKEY NVARCHAR2(10), 
  REFERENCEACCOUNTINGENTITY NVARCHAR2(64), 
  EARLIESTSHIPDATE DATE, 
  REQUIREDSHIPDATE DATE, 
  PROMISEDSHIPDATE DATE, 
  PLANNEDSHIPDATE DATE, 
  EARLIESTDELIVERYDATE DATE, 
  REQUIREDDELIVERYDATE DATE, 
  PROMISEDDELIVERYDATE DATE, 
  PLANNEDDELIVERYDATE DATE, 
  SHIPFROMADDRESSLINE5 NVARCHAR2(45), 
  SHIPFROMADDRESSLINE6 NVARCHAR2(45), 
  RETURNFROMPARTY NVARCHAR2(64), 
  TEMPERATURE NUMBER(5,2), 
  TRAILERKEY NVARCHAR2(20), 
  TRANSPORTATIONSTATUS NVARCHAR2(10), 
  VERIFIEDCLOSEDDATE DATE, 
  ACTUALDEPARTUREDATE DATE, 
  TRANSPORTATIONSERVICE NVARCHAR2(30), 
  PRONUMBER NVARCHAR2(32), 
  FREIGHTCHARGEBASEAMOUNT NUMBER(22,5), 
  FREIGHTCHARGEAMOUNT NUMBER(22,5), 
  TMFREIGHTCHARGECURRENCY NVARCHAR2(3), 
  FREIGHTCOSTBASEAMOUNT NUMBER(22,5), 
  FREIGHTCOSTAMOUNT NUMBER(22,5), 
  TMFREIGHTCOSTCURRENCY NVARCHAR2(3), 
  CARRIERROUTESTATUS NVARCHAR2(10) DEFAULT 'NEW', 
  ADDDATE DATE DEFAULT SYS_EXTRACT_UTC(SYSTIMESTAMP) , 
  ADDWHO NVARCHAR2(256) DEFAULT USER , 
  EDITDATE DATE DEFAULT SYS_EXTRACT_UTC(SYSTIMESTAMP) , 
  EDITWHO NVARCHAR2(256) DEFAULT USER , 
  NOTES NVARCHAR2(2000), 
  RETURNLOTS NVARCHAR2(1) DEFAULT '0', 
  CONSIGNEEKEY NVARCHAR2(15), 
  PRODUCTIONSTATUS NVARCHAR2(30), 
  EXPECTEDRCEIPTDATE VARCHAR2(30), 
  DEFAULT1 VARCHAR2(150), 
  DEFAULT2 VARCHAR2(100), 
  DEFAULT3 VARCHAR2(100), 
  DEFAULT4 VARCHAR2(100), 
  DEFAULT5 VARCHAR2(60), 
  DEFAULT6 VARCHAR2(100), 
  DEFAULT7 VARCHAR2(100), 
  DEFAULT8 VARCHAR2(100), 
  DEFAULT9 VARCHAR2(100), 
  DEFAULT10 VARCHAR2(100), 
  DEFAULT11 VARCHAR2(100), 
  DEFAULT12 VARCHAR2(100), 
  DEFAULT13 VARCHAR2(100), 
  DEFAULT14 VARCHAR2(100), 
  DEFAULT15 VARCHAR2(100), 
  DEFAULT16 VARCHAR2(100), 
  DEFAULT17 VARCHAR2(100), 
  DEFAULT18 VARCHAR2(100), 
  DEFAULT19 VARCHAR2(100), 
  DEFAULT20 VARCHAR2(100), 
  DEFAULT21 VARCHAR2(100), 
  DEFAULT22 VARCHAR2(100), 
  DEFAULT23 VARCHAR2(100), 
  DEFAULT24 VARCHAR2(100), 
  DEFAULT25 VARCHAR2(100), 
  DEFAULT26 VARCHAR2(100), 
  DEFAULT27 VARCHAR2(100), 
  CREATEDATE VARCHAR2(30), 
  WEIGHT NUMBER DEFAULT 0, 
  EXTERNPOKEY NVARCHAR2(30), 
  EXTERNALPOKEY2 NVARCHAR2(30)
)
/
create sequence API_RECEIPT_seq
minvalue 1
maxvalue 999999999999999999999999999999
start with 1
increment by 1
cache 20;

/
CREATE OR REPLACE TRIGGER API_RECEIPT_TRIGGER BEFORE INSERT ON API_RECEIPT FOR EACH ROW
DECLARE
  BEGIN
    IF :NEW.SERIALKEY IS NULL THEN
      SELECT API_RECEIPT_SEQ.NEXTVAL
      INTO   :NEW.SerialKey
      FROM   dual;
        END IF;
  END;
/
alter table API_RECEIPT add batchid nvarchar2(50);
alter table API_RECEIPT add  rtxeditflag nvarchar2(20) default 'P';
alter table API_RECEIPT add  rtxmsg nvarchar2(500);
/