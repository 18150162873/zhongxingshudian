/
create sequence api_ordersdetail_seq
minvalue 1
maxvalue 999999999999999999999999999999
start with 1
increment by 1
cache 20;

/
CREATE OR REPLACE TRIGGER api_orderdetail_TRIGGER BEFORE INSERT ON api_orderdetail FOR EACH ROW
DECLARE
  BEGIN
    IF :NEW.SERIALKEY IS NULL THEN
      SELECT api_ordersdetail_SEQ.NEXTVAL
      INTO   :NEW.SerialKey
      FROM   dual;
        END IF;
  END;
/