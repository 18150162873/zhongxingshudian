/
create sequence api_orders_seq
minvalue 1
maxvalue 999999999999999999999999999999
start with 1
increment by 1
cache 20;

/
CREATE OR REPLACE TRIGGER api_orders_TRIGGER BEFORE INSERT ON api_orders FOR EACH ROW
DECLARE
  BEGIN
    IF :NEW.SERIALKEY IS NULL THEN
      SELECT api_orders_SEQ.NEXTVAL
      INTO   :NEW.SerialKey
      FROM   dual;
        END IF;
  END;
/